    <section id="contact-link">
        <div class="contact-display">          
            <h1 class="display-4 "><b class="text-danger contact-me">Contact</b> Me</h1>
            <div class="row">
                <div class="col-6">
                    <form method="post"> 
                        @Html.AntiForgeryToken()
                        <div class="row">
                            <div class="col-6">
                                <input type="text" class="message-input" id="first-name" name="FirstName" placeholder="First Name" pattern="^[a-zA-Z''-'\s]{2,40}$" required minlength="2" maxlength="40" oninput="this.setCustomValidity('')" oninvalid="this.setCustomValidity('This input must only be letters and have more than one letter!')" />
                            </div>

                            <div class="col-6">
                                <input type="text" id="last-name" class="message-input" name="LastName" placeholder="Last Name" pattern="^[a-zA-Z''-'\s]{2,40}$" required minlength="2" maxlength="40" oninput="this.setCustomValidity('')" oninvalid="this.setCustomValidity('This input must only be letters and have more than one letter!')" />
                            </div>
                        </div>
                        
                        <div class="row">
                            <div class="col-6">
                                <input type="text" class="message-input" id="subject" name="Subject" placeholder="Subject" pattern="^[a-zA-Z''-'\s]{2,40}$" required minlength="2" maxlength="40" oninput="this.setCustomValidity('')" oninvalid="this.setCustomValidity('This input must only be letters and have more than one letter!')" />
                            </div>

                            <div class="col-6">
                                <input type="email" id="email" class="message-input" name="Email" placeholder="Email" />
                            </div>
                        </div>
                        
                        <div class="row">
                            <div class="col-12">
                                <textarea type="text" class="message-input" id="message" name="Message" placeholder="Message" required minlength="2" maxlength="600" pattern="^[a-zA-Z''-'\S]{2,600}$" oninput="this.setCustomValidity('')" oninvalid="this.setCustomValidity('This input must be greater than one character!')"></textarea>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-12">
                                <input type="submit" class="message-input" id="submit-message" value="Submit" />
                            </div>
                        </div>
                    </form>
                </div>

                <div class="col-6">
                    <div id="myCarousel" class="carousel slide" data-ride="carousel">
                        <ol class="carousel-indicators">
