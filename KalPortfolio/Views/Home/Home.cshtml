        <section id="contact-link">

            <h1 class="display-4 "><b class="text-danger contact-me">Contact</b> Me</h1>

            <div class="row">
                <div class="col-xl-6 col-12">
                    <form asp-action="Home" asp-controller="Home" method="post" id=messageform>
                        @Html.AntiForgeryToken()
                        <div class="row gy-2">
                            <div class="col-6">
                                <input type="text" asp-for="@Model.FirstName" class="message-input" id="first-name" placeholder="First Name" required pattern="^([a-zA-Z\\d!@@#$%^&*(),.?&quot;:{}|<>]]){8,25}$"/>
                                <span asp-validation-for="@Model.FirstName" class="text-danger"></span>
                            </div>

                            <div class="col-6">
                                <input type="text" asp-for="@Model.LastName" id="last-name" class="message-input" placeholder="Last Name" required pattern="^([a-zA-Z\\d!@@#$%^&*(),.?&quot;:{}|<>]]){8,25}$" />
                                <span asp-validation-for="@Model.LastName" class="text-danger"></span>
                            </div>

                            <div class="col-6">
                                <input type="text" asp-for="@Model.Subject" class="message-input" id="subject" placeholder="Subject" required pattern="^([a-zA-Z\\d!@@#$%^&*(),.?&quot;:{}|<>]]){8,25}$"/>
                                <span asp-validation-for="@Model.Subject" class="text-danger"></span>
                            </div>

                            <div class="col-6">
                                <input type="email" asp-for="@Model.Email" id="email" class="message-input" placeholder="Email" required pattern="^([a-zA-Z\\d!@@#$%^&*(),.?&quot;:{}|<>]]){8,25}$" />
                                <span asp-validation-for="@Model.Email" class="text-danger"></span>
                            </div>
                        </div>

                        <div class="row gy-3">
                            <div class="col-12">
                                <textarea type="text" asp-for="@Model.Message" class="message-input" rows="9" id="message" placeholder="Message" required></textarea>
                                <span asp-validation-for="@Model.Message" class="text-danger"></span>
                            </div>

                            <div class="col-12">
                                <button type="submit" class="fs-3" id="submit-message">Submit</button>
                            </div>
                        </div>
                    </form>
                </div>

                <div class="col-6">
                    <div id="about-me-carousel" class="carousel slide">
                        <div class="carousel-indicators">
                            <button type="button" data-bs-target="#about-me-carousel" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                            <button type="button" data-bs-target="#about-me-carousel" data-bs-slide-to="1" aria-label="Slide 2"></button>
                            <button type="button" data-bs-target="#about-me-carousel" data-bs-slide-to="2" aria-label="Slide 3"></button>
                        </div>
                        <div class="carousel-inner">
                            <div class="carousel-item active">
